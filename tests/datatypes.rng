<?xml version="1.0" encoding="UTF-8"?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0"
         datatypeLibrary="">
  <start>
    <element>
      <name ns="">token</name>
      <data type="token"/>
    </element>
    <element>
      <name ns="">token_abc</name>
      <value>abc</value>
    </element>
    <element>
      <name ns="">string</name>
      <data type="string"/>
    </element>
    <element>
      <name ns="">string_abc</name>
      <value type="string">abc</value>
    </element>
    <element>
      <name ns="">xsd_string</name>
      <data type="string" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
    </element>
    <element>
      <name ns="">xsd_string_abc</name>
      <value type="string" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">abc</value>
    </element>
    <element>
      <name ns="">xsd_double</name>
      <data type="double" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
    </element>
    <element>
      <name ns="">xsd_double_42</name>
      <value type="double" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">42</value>
    </element>
    <element>
      <name ns="">custom_foo</name>
      <data type="foo" datatypeLibrary="uri:custom-datatype-library"/>
    </element>
    <element>
      <name ns="">custom_foo_abc</name>
      <value type="foo" datatypeLibrary="uri:custom-datatype-library">abc</value>
    </element>
  </start>
</grammar>
